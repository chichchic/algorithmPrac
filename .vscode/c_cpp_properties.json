// includePath 오류 해결 https://stackoverflow.com/questions/17939930/finding-out-what-the-gcc-include-path-is

{
  "configurations": [
    {
      "name": "Mac",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/local/include",
        "/Library/Developer/CommandLineTools/usr/bin/../include/c++/v1",
        "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/usr/include",
        "/Library/Developer/CommandLineTools/usr/include"
      ],
      "defines": [],
      "macFrameworkPath": [],
      "compilerPath": "/usr/local/bin/gcc-10",
      "cStandard": "gnu17",
      "intelliSenseMode": "macos-gcc-x64",
      "cppStandard": "c++11"
    }
  ],
  "version": 4
}
